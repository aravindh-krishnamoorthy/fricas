)set break resume
)expose UnittestCount UnittestAux Unittest

testsuite "bugs2024"

testcase "simplifyExp"

testEquals("simplifyExp(2*2^a)", "2^(a+1)")
testEquals("simplifyExp(a*a^b)", "a^(b+1)")
